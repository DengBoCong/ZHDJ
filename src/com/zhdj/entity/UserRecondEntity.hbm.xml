<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhdj.entity.UserRecondEntity" table="user_recond" schema="zhdj">
        <id name="recondFlag">
            <column name="recond_flag" sql-type="int(11)" not-null="true"/>
        </id>
        <property name="userid">
            <column name="userid" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="recondTime">
            <column name="recond_time" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="recondContent">
            <column name="recond_content" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>